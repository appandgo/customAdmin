{
    "name": "Laravel Angular Admin",
    "description": "Laravel + Angularjs + Bootstrap + AdminLTE binded by Gulp workflow Admin Dashboard Boilerplate",
    "keywords": ["laravel", "angular", "bootstrap", "admin", "admin template", "ui-router", "api", "angular-bootstrap"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "tymon/jwt-auth": "0.5.*",
        "laravelangular/generators": "2.7.*",
        "dingo/api": "1.0.x@dev",
        "laravel/socialite": "3.0.0",
        "bican/roles": "dev-master",
        "mpociot/laravel-firebase-sync": "^1.0",
        "brozot/laravel-fcm": "^1.2",
        "barryvdh/laravel-cors": "^0.8.2",
        "ultraware/roles": "^5.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""

        ],
        "post-create-project-cmd": [
           "php -r \"unlink('code_of_conduct.md');\"",
           "php -r \"unlink('CHANGELOG.md');\"",
           "php -r \"unlink('.codeclimate.yml');\"",
           "php -r \"unlink('.travis.yml');\"",
           "php artisan key:generate",
           "php artisan jwt:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan optimize",
            "php artisan key:generate",
            "php artisan config:cache",
            "php artisan route:cache",
            "php artisan api:cache"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"

        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
